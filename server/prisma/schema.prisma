generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  user_id       Int       @id @default(autoincrement())
  email         String
  username      String    @unique
  current_start DateTime?
  records       records[]
}

model records {
  record_id  Int      @id @default(autoincrement())
  start_time DateTime
  end_time   DateTime
  author     Int
  user_id    users    @relation(fields: [author], references: [user_id])
}
